{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:vue/base",
        "standard"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "parser": "@typescript-eslint/parser",
        "sourceType": "module"
    },
    "plugins": [
        "vue",
        "@typescript-eslint"
    ],
    "rules": {
        "vue/valid-template-root": "error",
        "no-multiple-empty-lines": "error"
    }
}